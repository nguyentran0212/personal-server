# App identification (required)
APP_NAME=airflow
APP_DOMAIN=airflow
APP_PORT=8080

# Airflow-specific configuration
AIRFLOW_UID=50000
AIRFLOW_IMAGE_NAME=apache/airflow:3.1.0
